<h3>New User</h3>
<%= form_for @user do |f| %>
	<div class="form-group">
	<%=f.label :first_name%>
  <%= f.text_field :first_name, class: "form-control" %>
  </div>
  <div class="form-group">
  <%=f.label :last_name%>
  <%= f.text_field :last_name, class: "form-control" %>
  </div>
  <div class="form-group">
  <%=f.label :email%>
  <%= f.text_field :email, class: "form-control" %>
  </div>
  <%= f.submit "Submit" %>
<% end %>
